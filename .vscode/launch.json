{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "LoadbalancerApplication",
            "request": "launch",
            "mainClass": "loadbalancer.service.loadbalancer.LoadbalancerApplication",
            "projectName": "loadbalancer.service-loadbalancer-service"
        },
        {
            "type": "java",
            "name": "UserServiceApplication",
            "request": "launch",
            "mainClass": "user.service.userservice.UserServiceApplication",
            "projectName": "user-service"
        },
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Eureka-server-service",
            "request": "launch",
            "mainClass": "lab.microservice.namingserver.SpringBootMicroserviceEurekaNamingServerApplication",
            "projectName": "eureka-naming-server"
        },
        {
            "type": "java",
            "name": "random-number-service",
            "request": "launch",
            "mainClass": "random.service.randomnumber.RandomnumberApplication",
            "projectName": "random-number-service"
        },
        {
            "type": "java",
            "name": "User-history-service",
            "request": "launch",
            "mainClass": "com.example.demo.UserHistoryApplication",
            "projectName": "user-history"
        },
        {
            "type": "java",
            "name": "User-account-service1",
            "request": "launch",
            "mainClass": "user.service.userservice.UserServiceApplication",
            "projectName": "user-service",
            "vmArgs": [
                "-Dserver.port=8000"
            ]
        },
        {
            "type": "java",
            "name": "User-account-service2",
            "request": "launch",
            "mainClass": "user.service.userservice.UserServiceApplication",
            "projectName": "user-service",
            "vmArgs": [
                "-Dserver.port=8001"
            ]
        },
        {
            "type": "java",
            "name": "account-load-balancer",
            "request": "launch",
            "mainClass": "loadbalancer.service.loadbalancer.LoadbalancerApplication",
            "projectName": "loadbalancer-service",
            "vmArgs": [
                "-Dserver.port=8100"
            ]
        }
    ]
}